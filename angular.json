{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "blog": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/blog/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "crypto-js",
              "validator/lib/contains",
              "validator/lib/matches",
              "validator/lib/isLength",
              "validator/lib/isVariableWidth",
              "validator/lib/isURL",
              "validator/lib/isUppercase",
              "validator/lib/isUUID",
              "validator/lib/isSurrogatePair",
              "validator/lib/isSemVer",
              "validator/lib/isRgbColor",
              "validator/lib/isRFC3339",
              "validator/lib/isPostalCode",
              "validator/lib/isPort",
              "validator/lib/isPassportNumber",
              "validator/lib/isOctal",
              "validator/lib/isNumeric",
              "validator/lib/isMultibyte",
              "validator/lib/isMimeType",
              "validator/lib/isMagnetURI",
              "validator/lib/isMACAddress",
              "validator/lib/isLowercase",
              "validator/lib/isLocale",
              "validator/lib/isJWT",
              "validator/lib/isJSON",
              "validator/lib/isIdentityCard",
              "validator/lib/isISSN",
              "validator/lib/isISRC",
              "validator/lib/isISO31661Alpha3",
              "validator/lib/isISO31661Alpha2",
              "validator/lib/isISO8601",
              "validator/lib/isISIN",
              "validator/lib/isISBN",
              "validator/lib/isIP",
              "validator/lib/isIBAN",
              "validator/lib/isHexadecimal",
              "validator/lib/isHexColor",
              "validator/lib/isHalfWidth",
              "validator/lib/isHash",
              "validator/lib/isHSL",
              "validator/lib/isFullWidth",
              "validator/lib/isFQDN",
              "validator/lib/isEthereumAddress",
              "validator/lib/isEmail",
              "validator/lib/isEAN",
              "validator/lib/isDecimal",
              "validator/lib/isDataURI",
              "validator/lib/isCurrency",
              "validator/lib/isCreditCard",
              "validator/lib/isByteLength",
              "validator/lib/isBtcAddress",
              "validator/lib/isBoolean",
              "validator/lib/isBase64",
              "validator/lib/isBase32",
              "validator/lib/isBIC",
              "validator/lib/isAscii",
              "validator/lib/isAlphanumeric",
              "validator/lib/isAlpha",
              "validator/lib/isDivisibleBy",
              "validator/lib/isLatLong",
              "validator/lib/isMobilePhone",
              "validator/lib/isMongoId",
              "chart.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": false,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "50000kb",
                  "maximumError": "30mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1mb",
                  "maximumError": "4mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "media",
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "progress": true,
              "aot": true
            },
            "qas": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": false,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "50000kb",
                  "maximumError": "30mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1mb",
                  "maximumError": "4mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.qas.ts"
                }
              ],
              "outputHashing": "media",
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "progress": true,
              "aot": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "blog:build:production"
            },
            "development": {
              "browserTarget": "blog:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "blog:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "serverless": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/blog/serverless",
            "main": "serverless.ts",
            "tsConfig": "tsconfig.serverless.json",
            "inlineStyleLanguage": "scss"
          },
          "configurations": {
            "production": {
              "optimization": {
                "scripts": true
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "media",
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "progress": true
            },
            "qas": {
              "optimization": {
                "scripts": true
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.qas.ts"
                }
              ],
              "outputHashing": "media",
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "progress": true
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false,
              "outputHashing": "media",
              "namedChunks": true,
              "progress": true
            }
          },
          "defaultConfiguration": "production"
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/blog/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "inlineStyleLanguage": "scss"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "blog:build:development",
              "serverTarget": "blog:server:development"
            },
            "production": {
              "browserTarget": "blog:build:production",
              "serverTarget": "blog:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "blog:build:production",
              "serverTarget": "blog:server:production"
            },
            "development": {
              "browserTarget": "blog:build:development",
              "serverTarget": "blog:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "defaultProject": "blog"
}
