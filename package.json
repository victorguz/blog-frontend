{
  "name": "blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run blog:serve-ssr",
    "serve:ssr": "node dist/blog/server/main.js",
    "build:ssr": "ng build && ng run blog:server",
    "prerender": "ng run blog:prerender",
    "start:dev": "ng serve --configuration development",
    "start:qas": "ng serve --configuration qas",
    "start:prod": "ng serve --configuration production",
    "build:dev": "ng build --configuration development --aot && ng run blog:serverless:development",
    "build:qas": "ng build --configuration qas --aot && ng run blog:serverless:qas",
    "build:prod": "ng build --configuration production --aot && ng run blog:serverless:production",
    "logs:dev": "serverless logs -t -f api --stage dev",
    "logs:qas": "serverless logs -t -f api --stage qas",
    "logs:prod": "serverless logs -t -f api --stage prd",
    "deploy:dev": "npm run build:dev && serverless deploy function -f api --stage dev",
    "deploy:qas": "npm run build:qas && serverless deploy function -f api --stage qas",
    "deploy:prod": "npm run build:prod && serverless deploy function -f api --stage prd",
    "deploy:dev:force": "npm run build:dev && serverless deploy --stage dev",
    "deploy:qas:force": "npm run build:qas && serverless deploy --stage qas",
    "deploy:prod:force": "npm run build:prod && serverless deploy --stage prd"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/platform-server": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@ng-toolkit/serverless": "^8.1.0",
    "@nguniversal/express-engine": "^12.1.3",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "bootstrap": "^5.2.0",
    "class-validator": "^0.13.2",
    "clean-css": "^5.3.1",
    "commander": "^9.4.0",
    "crypto-js": "^4.1.1",
    "express": "^4.15.2",
    "he": "^1.2.0",
    "html-minifier": "^4.0.0",
    "lower-case": "^2.0.2",
    "no-case": "^3.0.4",
    "param-case": "^3.0.4",
    "relateurl": "^0.2.7",
    "rxjs": "^6.6.0",
    "secure-web-storage": "^1.0.2",
    "serverless-http": "^3.0.2",
    "source-map": "^0.7.4",
    "time-ago-pipe": "^1.3.2",
    "tslib": "^2.3.0",
    "uglify-js": "^3.16.3",
    "upper-case": "^2.0.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.0",
    "@nguniversal/builders": "^12.1.3",
    "@types/aws-serverless-express": "^3.3.5",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.7.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-offline": "^9.2.0",
    "serverless-stage-manager": "^1.0.5",
    "typescript": "~4.3.5"
  }
}
